{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "source": "gentoo2",
        "color": {
            "1": "red",
            "2": "white"
        }
    },
    "display": {
        "color": {
            "separator": "red"
        },
        "separator": " | ",
        "constants": [
            "X------------X---------------------------------------------X"
        ]
    },
    "modules": [
        {
            "type": "kernel",
            "key": "Gentoo Linux",
            "keyColor": "red"
        },
        {
            "type": "custom",
            "format": "{$1}",
            "outputColor": "separator"
        },
        {
            "type": "display",
            "key": "   Display  ",
            "keyColor": "red"
        },
        {
            "type": "cpu",
            "key": "   CPU      ",
            "keyColor": "red"
        },
        {
            "type": "gpu",
            "key": "   GPU      ",
            "keyColor": "red"
        },
        {
            "type": "memory",
            "key": "   RAM      ",
            "keyColor": "red"
        },
        {
            "type": "swap",
            "key": "   SWAP     ",
            "keyColor": "red"
        },
        {
            "type": "disk",
            "key": "   Disk     ",
            "keyColor": "red"
        },
        {
            "type": "custom",
            "format": "{$1}",
            "outputColor": "separator"
        },
    ]
}
